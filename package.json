{
  "name": "japanese-holidays",
  "version": "0.1.0",
  "description": "日本の祝日を扱うための TypeScript ライブラリ",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "japanese",
    "holidays",
    "japan",
    "祝日",
    "typescript"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./light": {
      "types": "./dist/light.d.ts",
      "default": "./dist/light.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "test": "bash -c 'node --test src/**/*.test.ts'",
    "test:ci": "bash -c 'node --test dist/**/*.test.js'",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.10",
    "typescript": "^5.9.0"
  }
}
