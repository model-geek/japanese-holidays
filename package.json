{
  "name": "@modelgeek/japanese-holidays",
  "version": "0.1.0",
  "description": "日本の祝日を扱うための TypeScript ライブラリ",
  "type": "module",
  "license": "MIT",
  "author": "Model Geek",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/model-geek/japanese-holidays.git"
  },
  "homepage": "https://github.com/model-geek/japanese-holidays#readme",
  "bugs": {
    "url": "https://github.com/model-geek/japanese-holidays/issues"
  },
  "keywords": [
    "japanese",
    "holidays",
    "japan",
    "祝日",
    "typescript"
  ],
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "format": "echo \"TODO: add format script\"",
    "generate:check-all-test": "tsx scripts/generate-check-all-test.ts",
    "lint": "echo \"TODO: add lint script\"",
    "test": "TZ=UTC node --import tsx --test $(find src scripts test -name '*.test.ts')",
    "tidy": "npm run format && npm run lint && npm run typecheck && npm test",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.10",
    "tsx": "^4.21.0",
    "typescript": "^5.9.0"
